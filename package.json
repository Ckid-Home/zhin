{
    "name": "zhin-workspace",
    "private": true,
    "version": "3.0.2",
    "description": "zhin机器人开发框架",
    "repository": {
        "url": "https://github.com/zhinjs/zhin"
    },
    "scripts": {
        "start": "pnpm -C test start",
        "dev": "pnpm -C test dev",
        "format": "prettier packages --write",
        "build": "pnpm -r run build",
        "test": "pnpm -C test test",
        "release": "pnpm changeset",
        "clean": "rm -rf ./node_modules",
        "pub": "pnpm -r publish --access public --no-git-checks --registry https://registry.npmjs.org",
        "bump": "pnpm changeset version",
        "docs:dev": "vitepress dev docs --port 5566",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs"
    },
    "packageManager": "pnpm@9.0.2",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "keywords": [
        "qq",
        "group",
        "bot"
    ],
    "author": "凉菜",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.14.11",
        "latest-version": "^5.0.0",
        "less": "^4.2.0",
        "prettier": "^3.1.0",
        "rimraf": "^5.0.5",
        "semver": "^7.5.4",
        "tsc-alias": "^1.8.8",
        "typescript": "^5.2.2",
        "vitepress": "1.3.4",
        "vue": "latest"
    },
    "dependencies": {
        "@changesets/cli": "^2.27.7",
        "@element-plus/icons-vue": "^2.3.1",
        "element-plus": "^2.7.8",
        "markdown-it": "^14.1.0"
    }
}
